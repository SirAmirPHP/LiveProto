{
    "name": "taknone/liveproto",
    "license": "MIT",
    "description": "An async , pure PHP MTProto Telegram client",
    "type": "library",
    "keywords": [
        "telegram",
        "client",
        "api",
        "php",
        "liveproto",
        "live",
        "proto",
        "telegram-client",
        "mtproto",
        "async"
    ],
    "autoload": {
        "psr-4": {
            "Tak\\Liveproto\\": "src"
        },
        "files": [
            "src/loader.php"
        ]
    },
