        "ext-filter": "*",
        "ext-hash": "*",
        "ext-zlib": "*",
        "ext-fileinfo": "*",
        "amphp/amp": "^3.1.0",
        "amphp/websocket-client": "^2.0.1",
        "amphp/socket": "^2.3.1",
        "amphp/file": "^3.2.0",
        "amphp/mysql": "^3",
        "phpseclib/phpseclib": "^3.0.46",
        "taknone/attributes": "*",
        "taknone/bootstrapper": "*"
